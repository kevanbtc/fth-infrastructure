{
  "filter_paths": [
    "lib",
    "test",
    "script"
  ],
  "exclude_informational": false,
  "exclude_low": false,
  "exclude_medium": false,
  "exclude_high": false,
  "solc_remaps": [
    "openzeppelin-contracts/=lib/openzeppelin-contracts/",
    "forge-std/=lib/forge-std/src/"
  ],
  "compilation_skip": true,
  "detectors_to_exclude": [
    "pragma",
    "solc-version",
    "naming-convention"
  ]
}